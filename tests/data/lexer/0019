[a.b.c]            # this is best practice
[ d.e.f ]          # same as [d.e.f]
[ g .  h  . i ]    # same as [g.h.i]
[ j . "ʞ" . 'l' ]  # same as [j."ʞ".'l']

---
OPEN_BRACKET  '['
BARE_KEY      'a'
PERIOD        '.'
BARE_KEY      'b'
PERIOD        '.'
BARE_KEY      'c'
CLOSE_BRACKET ']'
WHITESPACE    '            '
COMMENT       '# this is best practice'
LINE_END      '\n'
OPEN_BRACKET  '['
WHITESPACE    ' '
BARE_KEY      'd'
PERIOD        '.'
BARE_KEY      'e'
PERIOD        '.'
BARE_KEY      'f'
WHITESPACE    ' '
CLOSE_BRACKET ']'
WHITESPACE    '          '
COMMENT       '# same as [d.e.f]'
LINE_END      '\n'
OPEN_BRACKET  '['
WHITESPACE    ' '
BARE_KEY      'g'
WHITESPACE    ' '
PERIOD        '.'
WHITESPACE    '  '
BARE_KEY      'h'
WHITESPACE    '  '
PERIOD        '.'
WHITESPACE    ' '
BARE_KEY      'i'
WHITESPACE    ' '
CLOSE_BRACKET ']'
WHITESPACE    '    '
COMMENT       '# same as [g.h.i]'
LINE_END      '\n'
OPEN_BRACKET  '['
WHITESPACE    ' '
BARE_KEY      'j'
WHITESPACE    ' '
PERIOD        '.'
WHITESPACE    ' '
BASIC_STRING  '"ʞ"'
WHITESPACE    ' '
PERIOD        '.'
WHITESPACE    ' '
LITERAL_STRING "\'l\'"
WHITESPACE    ' '
CLOSE_BRACKET ']'
WHITESPACE    '  '
COMMENT       "# same as [j.\"ʞ\".\'l\']"
LINE_END      '\n'
